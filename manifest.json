{
    "manifest_version": 2,
    "name": "Logged Out Twitter",
    "version": "0.1.0", 
    "description": "Removes Twitter pop-ups that force you to log in.",
    "icons": {
        "48": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://twitter.com/*", "*://*.twitter.com/*"],
            "js": ["removePopups.js"]
        }
    ] 
}

  